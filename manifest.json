{
  "name": "Audica Player",
  "short_name": "audicaP",
  "version": "0.12",
  "description": "",
  "app": {
    "background": {
      "scripts": [
        "js/background.js"
      ]
    }
  },
  "icons": {
    "128": "images/icon128.png",
    "48": "images/icon48.png",
    "16": "images/icon16.png"
  },
  "offline_enabled": false,
  "permissions": [
    "storage",
    "fileBrowserHandler",
    "unlimitedStorage",
    "<all_urls>",
    "http://*/*",
    "https://*/*"
  ],
  "manifest_version": 2
}
