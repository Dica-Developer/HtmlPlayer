{
  "name": "Audica Player",
  "version": "0.10",
  "description": "",
  "app": {
    "launch": {
      "local_path": "main.html",
      "container": "tab"
    }
  },
  "content_scripts": [{
    "matches": ["https://one.ubuntu.com/phones/creds"],
    "js": ["js/3rd/ubuntuone_authentication.js"],
    "run_at": "document_end"
  }, {
    "matches": ["http://www.google.com/robots.txt*"],
    "js": ["js/3rd/oauth2/oauth2_inject.js"],
    "run_at": "document_start"
  }],
  "icons": {
    "128": "images/icon128.png",
    "48": "images/icon48.png",
    "16": "images/icon16.png"
  },
  "offline_enabled": false,
  "options_page": "options/index.html",
  "permissions": ["fileBrowserHandler", "unlimitedStorage", "https://one.ubuntu.com/music/api/1.0/*", "https://accounts.google.com/o/oauth2/token", "https://streaming.one.ubuntu.com/rest/*", "https://*.web.cddbp.net/webapi/xml/1.0/", "https://www.google.com/accounts/clientlogin", "https://play.google.com/music/services/*", "https://play.google.com/music/*"],
  "manifest_version": 2,
  "content_security_policy": "default-src *; style-src 'self'; script-src 'self'; object-src 'self'; connect-src * ; img-src *"
}