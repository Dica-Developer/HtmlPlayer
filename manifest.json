{
  "name": "Audica Player",
  "version": "0.9",
  "description": "",
  "app": {
    "launch": {
      "local_path": "main.html",
      "container": "tab"
    }
  },
  "content_scripts": [
    {
      "matches": ["https://one.ubuntu.com/phones/creds"],
      "js": ["js/ubuntuone_authentication.js"],
      "run_at": "document_end"
    },
    {
      "matches": ["http://www.google.com/robots.txt*"],
      "js": ["oauth2/oauth2_inject.js"],
      "run_at": "document_start"
    }
  ],
  "icons": {
	  "128": "images/icon128.png",
	  "48": "images/icon48.png",
	  "16": "images/icon16.png"
	},
  "options_page": "options/index.html",
  "permissions": ["fileBrowserHandler", "unlimitedStorage", "https://streaming.one.ubuntu.com/rest/*", "https://accounts.google.com/o/oauth2/token"]
}

